<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.loginController">
   <children>
      <AnchorPane layoutX="200.0" layoutY="53.0" prefHeight="268.0" prefWidth="200.0">
         <children>
            <Label layoutX="66.0" layoutY="21.0" text="login System" />
            <TextField fx:id="emailText" layoutX="26.0" layoutY="51.0" onAction="#emailTextonAction" />
            <PasswordField fx:id="passText" layoutX="26.0" layoutY="100.0" onAction="#passTextonAction" />
            <Button fx:id="btnLogin" layoutX="79.0" layoutY="140.0" mnemonicParsing="false" onAction="#btnLoginonAction" text="login" />
            <Label layoutX="58.0" layoutY="193.0" text="Login som guest:" />
            <Button fx:id="btnGuest" layoutX="65.0" layoutY="221.0" mnemonicParsing="false" onAction="#btnGuestonAction" text="Guest login" />
         </children>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </AnchorPane>
   </children>
</AnchorPane>
