      <Button layoutX="647.0" layoutY="264.0" mnemonicParsing="false" onAction="#DeleteButtonAction" prefHeight="26.0" prefWidth="204.0" text="Delete" />
      <Button layoutX="767.0" layoutY="394.0" mnemonicParsing="false" onAction="#goBackBtnHandler" prefHeight="26.0" prefWidth="82.0" text="Back" />
      <PasswordField fx:id="proPassword" layoutX="645.0" layoutY="168.0" prefHeight="18.0" prefWidth="204.0" promptText="Password">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></PasswordField>
   </children>
</AnchorPane>
