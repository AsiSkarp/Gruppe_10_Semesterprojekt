            <Font name="System Italic" size="12.0" />
         </font></TextField>
      <Label layoutX="646.0" layoutY="61.0" text="Insert the following information" textFill="WHITE" />
      <Button layoutX="392.0" layoutY="14.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="82.0" text="Search" />
