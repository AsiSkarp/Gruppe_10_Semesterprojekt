          <TableColumn fx:id="proTableName" onEditCommit="#updateName" prefWidth="131.0" text="Name" />
          <TableColumn fx:id="proTableEmail" editable="false" prefWidth="263.0" text="E-mail" />
            <TableColumn fx:id="proTablePassword" onEditCommit="#updatePassword" prefWidth="175.0" text="Password" />
        </columns>
      </TableView>
      <TextField layoutX="17.0" layoutY="14.0" prefHeight="26.0" prefWidth="370.0" promptText="Search" />
      <TextField fx:id="proName" layoutX="645.0" layoutY="96.0" prefHeight="26.0" prefWidth="204.0" promptText="Name">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></TextField>
      <TextField fx:id="proEmail" layoutX="645.0" layoutY="133.0" prefHeight="26.0" prefWidth="204.0" promptText="E-mail">
